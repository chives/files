<p>
    <a href="{{ path('index') }}">Index</a>
</p>
{% if message is not null %}
    <p>{{ message|nl2br|raw }}</p>
{% endif %}
{% form_theme form '@Files/formTheme.html.twig' %}
{{ form_start(form) }}
    {{ form_rest(form) }}
    <button>Submit</button>
{{ form_end(form) }}
<p>FSi\Tests\App\Entity\FileEntity configuration:</p>
<br/>
{% for field in configuration %}
    {% for key, value in field %}
        <p>{{ key }}: {{ value }}</p>
    {% endfor %}
    {% if not loop.last %}<br/>{% endif %}
{% endfor %}
