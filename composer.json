{
    "name": "fsi/files",
    "license": "proprietary",
    "type": "library",
    "description": "A library for handling file upload and storage",
    "autoload": {
        "psr-4": {
            "FSi\\Component\\Files\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FSi\\Tests\\": "tests/_support",
            "FSi\\Tests\\App\\": "tests/_support/project/src",
            "FSi\\Tests\\Component\\Files\\": [
                "tests/functional",
                "tests/unit"
            ]
        }
    },
    "require": {
        "php": "^7.4",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-intl": "*",
        "beberlei/assert": "^3.2",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "ramsey/uuid": "^3.8|^4.0"
    },
    "require-dev": {
        "ext-pdo_sqlite": "*",
        "codeception/codeception": "^3.1",
        "doctrine/doctrine-bundle": "^1.11",
        "doctrine/orm": "^2.6",
        "guzzlehttp/psr7": "^1.5",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/flysystem-memory": "^1.0",
        "monolog/monolog": "^1.25",
        "oneup/flysystem-bundle": "^3.1",
        "phpstan/phpstan": "^0.12.0",
        "phpstan/phpstan-beberlei-assert": "^0.12.0",
        "phpstan/phpstan-doctrine": "^0.12.0",
        "phpstan/phpstan-phpunit": "^0.12.0",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/config": "^4.3",
        "symfony/dependency-injection": "^4.3",
        "symfony/form": "^4.3",
        "symfony/framework-bundle": "^4.3",
        "symfony/http-foundation": "^4.3",
        "symfony/http-kernel": "^4.3",
        "symfony/routing": "^4.3",
        "symfony/translation": "^4.3",
        "symfony/twig-bundle": "^4.3",
        "symfony/validator": "^4.3",
        "twig/twig": "^2.12"
    },
    "scripts": {
        "sniffer": "vendor/bin/phpcs",
        "stan": "vendor/bin/phpstan  --ansi analyse -l 8 -c phpstan.neon src tests",
        "cept": "vendor/bin/codecept --ansi",
        "func": "vendor/bin/codecept --ansi run functional",
        "unit": "vendor/bin/codecept --ansi run unit",
        "cl": "tests/_support/project/bin/console --ansi",
        "db": [
            "tests/_support/project/bin/console --ansi doctrine:schema:drop --force",
            "tests/_support/project/bin/console --ansi doctrine:database:create",
            "tests/_support/project/bin/console --ansi doctrine:schema:create"
        ]
    },
    "config": {
        "bin-dir": "vendor/bin",
        "preferred-install": {
            "*": "dist"
        },
        "process-timeout": 0,
        "sort-packages": true
    }
}
